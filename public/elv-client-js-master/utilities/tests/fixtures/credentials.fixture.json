[
  {
    "path_matchers": [".*"],
    "remote_access": {
      "protocol": "s3",
      "platform": "aws",
      "path": "TestBucket/",
      "storage_endpoint": {
        "region": "us-west-1"
      },
      "cloud_credentials": {
        "access_key_id": "MY_KEY_ID",
        "secret_access_key": "MY_SECRET"
      }
    }
  }
]